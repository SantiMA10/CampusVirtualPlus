{
  "manifest_version": 2,
  "name": "CampusVirtual+",
  "version": "1.4.0",
  "options_ui":
  {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.campusvirtual.uniovi.es/index.php*", "https://www.campusvirtual.uniovi.es/",
        "https://www.campusvirtual.uniovi.es/my/"
      ],
      "js": ["content.js"]
    }
  ]
}