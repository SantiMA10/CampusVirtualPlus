{
  "manifest_version": 2,
  "name": "CampusVirtual+",
  "version": "1.0.0",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.campusvirtual.uniovi.es/index.php*", "https://www.campusvirtual.uniovi.es/"
      ],
      "js": ["content.js"]
    }
  ]
}